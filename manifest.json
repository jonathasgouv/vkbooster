{
	"manifest_version": 2,
	"name": "VK Member Blocking",
	"author": "JÃ´nathas Gouveia",
	"version": "0.5",
	"content_scripts": [
		{
			"matches": [
				"*://vk.com/*"
			],
			"js": [
				"content.js"
			]
		}
	],
	"browser_action": {
		"default_icon": "vk-logo.png",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": [
			"background.js"
		]
	},
	"permissions": [
		"storage"
	]
}
